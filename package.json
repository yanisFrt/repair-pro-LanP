{
  "name": "cnova-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "clean": "turbo run clean && rm -rf node_modules",
    "repair-pro:dev": "turbo run dev --filter=repair-pro",
    "repair-pro:build": "turbo run build --filter=repair-pro",
    "repair-pro:start": "turbo run start --filter=repair-pro",
    "codesnova:dev": "turbo run dev --filter=codesnova-landing-page",
    "codesnova:build": "turbo run build --filter=codesnova-landing-page",
    "codesnova:start": "turbo run start --filter=codesnova-landing-page"
  },
  "devDependencies": {
    "turbo": "^2.3.3"
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=20.0.0"
  }
}
